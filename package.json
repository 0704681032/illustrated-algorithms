{
  "name": "illustrated-algorithms",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skidding/illustrated-algorithms.git"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.8.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build:babel-plugin": "babel babel-plugin-trace-context/src -d babel-plugin-trace-context/lib",
    "test": "xo",
    "dev": "npm run build:babel-plugin && next"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "extends": "xo-react",
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": 0,
      "object-curly-spacing": 0,
      "react/jsx-closing-bracket-location": [
        1,
        "tag-aligned"
      ],
      "react/jsx-no-bind": 0
    },
    "overrides": [
      {
        "files": "algorithms/*.js",
        "rules": {
          "no-unused-vars": 0
        }
      }
    ]
  },
  "dependencies": {
    "babel-template": "^6.16.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.4.0",
    "next": "^2.0.0-beta.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
